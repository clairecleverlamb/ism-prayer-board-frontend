{
    "rewrites": [
      {
        "source": "/auth/:match*",
        "destination": "https://prayer-board-backend-bcece8a2a0ee.herokuapp.com/auth/:match*"
      },
      {
        "source": "/api/:match*",
        "destination": "https://prayer-board-backend-bcece8a2a0ee.herokuapp.com/api/:match*"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "s-maxage=1, stale-while-revalidate" },
          { "key": "Vary", "value": "User-Agent" }
        ]
      }
    ]
  }
  